{
  "name": "Speech Input",
  "author": "G100",
  "version": "1.1",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/16.png",
      "32": "images/32.png",
      "48": "images/48.png",
      "128": "images/128.png"
    }
  },
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [{
    "matches": ["*://*/*"],
    "js": ["content_script.js"]
  }],
  "manifest_version": 2
}
